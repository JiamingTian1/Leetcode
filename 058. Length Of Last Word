class Solution:
    def lengthOfLastWord(self, s):
        """
        :type s: str
        :rtype: int
        """
        s1=s.split(" ")
        i=1
        a=0
        while i<=len(s1):
            if len(s1[-i])==0:
                i+=1
            else:
                a=len(s1[-i])
                break
        
        
        return a
        
